{
  "name": "fleek-chatbox-widget",
  "version": "0.1.0",
  "description": "A standalone chatbox widget for Fleek agents",
  "main": "dist/chatbox.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "start": "npm run build && npx live-server --open=test/index.html",
    "dev": "rollup -c --watch & npx live-server --open=test/index.html",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --apply .",
    "lint:check": "biome lint .",
    "check": "biome check --apply .",
    "check:ci": "biome ci ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "preact": "^10.26.4"
  }
}
